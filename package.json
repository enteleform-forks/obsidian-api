{
    "name": "obsidian",
    "type": "module",
    "version": "1.4.11",
    "description": "Type definitions for the latest Obsidian API (https://obsidian.md)",
    "keywords": ["obsdmd"],
    "homepage": "https://obsidian.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/obsidianmd/obsidian-api.git"
    },
    "license": "MIT",
    "main":   "./index.cjs",
    "module": "./index.mjs",
    "types": "obsidian.d.ts",
    "exports": {
      ".": {
        "require": "./index.cjs",
        "import":  "./index.mjs"
      },
      "./CJS": {
        "require": "./index.cjs",
        "import":  "./index.cjs"
      },
      "./ESM": {
        "require": "./index.mjs",
        "import":  "./index.mjs"
      }
    },
    "dependencies": {
        "@types/codemirror": "5.60.8",
        "moment": "2.29.4"
    },
    "peerDependencies": {
        "@codemirror/state": "^6.0.0",
        "@codemirror/view": "^6.0.0"
    }
}
